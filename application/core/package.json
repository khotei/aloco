{
  "name": "core",
  "version": "0.0.1",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "codegen": "ts-node src/common/scripts/codegen.ts",
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch --preserveWatchOutput",
    "seed": "ts-node src/common/scripts/seed.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "tscheck": "tsc --noEmit",
    "lint": "eslint --fix",
    "test": "node -r ts-node/register --test src/**/*.spec.ts",
    "test:e2e": "node -r ts-node/register --test src/**/*.e2e-spec.ts",
    "clean": "rm -rf node_modules && rm -rf .turbo && rm -rf dist && rm -rf ./src/__generated__"
  },
  "dependencies": {
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@stream-io/node-sdk": "^0.4.1",
    "apollo-server-express": "^3.13.0",
    "bull": "^4.16.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "graphql": "^16.9.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.16.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@faker-js/faker": "^8.4.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.17",
    "@types/node": "^22.3.0",
    "@types/passport-jwt": "^4.0.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^3.1.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1"
  }
}
